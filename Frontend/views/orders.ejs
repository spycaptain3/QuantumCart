      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css"
    />
    <script src="https://cdnjs.cloudflare.com/ajax/libs/html2pdf.js/0.10.1/html2pdf.bundle.min.js"></script>
    
  </head>
  <body>
    <section id="header">
      <header class="header-area">
        <!-- site-navbar start -->
        <div class="navbar-area">
          <nav class="site-navbar">
            <!-- site logo -->
            <a href="/"><img src="./Frontend/img/logo.png" class="logo" /></a>

            <!-- site menu/nav -->
            <ul>
              <li>
                <a href="/">home</a>
              </li>
              <li><a href="/shop">shop</a></li>
              <li><a href="/orders" style="text-decoration: underline; color: #49b293">orders</a></li>
              <li><a href="/auth" id="nav_auth">Authenticate</a></li>
              <li><a id="nav_logout">Logout</a></li>
              <li>
                <a href="/cart"
                  ><i
                    style="color: black"
                    class="fa fa-shopping-bag"
                    aria-hidden="true"
                  ></i
                ></a>
              </li>
            </ul>

            <!-- nav-toggler for mobile version only -->
            <button class="nav-toggler">
              <span></span>
            </button>
          </nav>
        </div>
        <!-- navbar-area end -->
      </header>
    </section>
    <div class="container" id="notificationContainer" style="z-index: 3"></div>
    <section id="page-header" class="about-header">
      <h2>#order_hub</h2>
      <p>"Streamlined Orders, Seamless Delivery"</p>
    </section>

    <section id="cart" class="section-p1">
      <div class="order-container">
        <table class="responsive-table">
          <thead>
            <tr>
              <th scope="col">OrderId</th>
              <th scope="col">Total</th>
              <th scope="col">Delievery Location</th>
              <th scope="col">Order Status</th>
              <th scope="col">Cancel</th>
              <th scope="col">Reciept</th>
            </tr>
          </thead>
          <tbody id="prev-orders"></tbody>
        </table>
      </div>
    </section>

    <footer class="section-p1">
      <div class="col">
        <img class="logo" src="./Frontend/img/logo.png" alt="" />
        <h4>Contact</h4>
        <p><strong>Address:</strong>Shiva Enclave, Ghaziabad</p>
        <p><strong>Phone:</strong>+91 7082285866</p>
        <p><strong>Hours:</strong>10:00 - 18:00, Mon - Sat</p>
      </div>

      <div class="col">
        <h4>About</h4>
        <a href="#">About us</a>
        <a href="#">Delivery Information</a>
        <a href="#">Privacy Policy</a>
        <a href="#">Terms & Conditions</a>
        <a href="#">Contact us</a>
      </div>
      <div class="col">
        <h4>About</h4>
        <a href="/profile">Account</a>
        <a href="/login">Sign In</a>
        <a href="/cart">View Cart</a>
        <a href="/help">Help</a>
